{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "@next/next/no-async-client-component": "off", // 비동기 클라이언트 컴포넌트를 사용하는 것을 허용
    "@typescript-eslint/no-unused-vars": "off", // 사용되지 않는 변수가 있을 때 경고 또는 오류를 발생시키지 않도록 함
    "@typescript-eslint/no-empty-interface": "off" // 빈 인터페이스를 사용하는 것에 대해 경고 또는 오류를 발생시키지 않도록 함
  }
}
